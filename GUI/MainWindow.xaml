<Window x:Class="GUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI"
        mc:Ignorable="d"
        Icon="pack://application:,,,/Image/Logo.jpg"
        Title="Luxury Yacht Diesel" Height="450" Width="800"
        ResizeMode="NoResize" Loaded="Window_Loaded">
    <Grid x:Name="MainGrid">
        <TabControl Margin="1">
            <TabItem x:Name="DieselTab" 
                     Header="Salg af DIESEL">
                <Grid Background="White" />
            </TabItem>
            <TabItem x:Name="CustomerTap"
                     Header="Kunde">
                <Grid Background="White" />
            </TabItem>
            <TabItem x:Name="SupplierTap" 
                     Header="Leverandør">
                <Grid Background="White" />
            </TabItem>
            <TabItem x:Name="DailyPriceTap" 
                     Header="Dagspris">
                <Grid Background="White" />
            </TabItem>
        </TabControl>

    </Grid>
</Window>
